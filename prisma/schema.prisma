// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Test {
  id Int @id @default(autoincrement())
  email String @unique
  name String?
}

model Room {
  id Int @id @default(autoincrement())
  roomName String
  date DateTime
  avaliability Boolean @default(false)
  price Int
  maxPeople Int
  detail String 
}
